# Replace YOUR_DOMAIN with your actual domain (e.g., doodle.creativemornings.com)
innovation.makesantafe.org {
    reverse_proxy app:8080
    
    # Enable compression
    encode gzip zstd
    
    # Security headers
    header {
        X-Content-Type-Options nosniff
        X-Frame-Options DENY
        Referrer-Policy strict-origin-when-cross-origin
    }
}
